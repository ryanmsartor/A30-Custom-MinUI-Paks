	if menu_state=="intro" then
		draw_intro()
	end
	if menu_state=="title" then
		draw_title()
	end
	if menu_state=="highscore" then
		draw_highscore()
	end
	if menu_state=="options" then
		draw_options()
	end
	draw_loading()
	if menu_fade_in then
	 local t=min(8,ceil(mcounter/4))
		set_color_fade(t,true)
	end
end

