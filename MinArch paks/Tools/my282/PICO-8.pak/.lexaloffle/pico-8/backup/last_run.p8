function print_bignumber(x,y,s)
	local tbl={}
	for i=1,#s do
		add(tbl,tonum(s[i]))
	end
	for i=0,#tbl-1 do
	 local _sx=tbl[i+1]%5
	 local _sy=tbl[i+1]\5==1 and 16 or 0
	 spr(_sx+235+_sy,x+8*i,y)
	end
end

