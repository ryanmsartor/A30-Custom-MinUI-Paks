function update_menu()
	if menu_state=="intro" then
		update_intro()
	else 
		if menu_state=="title" then
			update_title()
		else 
			if menu_state=="highscore" then
				update_highscore()
			else
				if menu_state=="options" then
					update_options()
				end
			end
		end
	end
	update_loading()
end

