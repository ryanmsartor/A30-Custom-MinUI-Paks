function init_builds()
 dclr"builds,distfact,c_x,c_y,c_mx,c_my,build_active,build_t,build_act_time|{},0.001,64,112,0.5,0.5,false,0,0"
 create_pillars()
end

function activate_builds()
	dclr"build_active|true"
	build_act_time=time()
end

function add_build(x,y,h)
	local _b={x=x,y=y,h=h,dx=0,dy=0}
	add(builds,_b)
end

function create_pillars()
	for i=0,4 do
  local _s=i*32
  local _x=0
  add_build(  0+_x*2,16+_s,8)
 	add_build( 32+_x,0 +_s,8)
	 add_build( 64,16+_s,8)
	 add_build( 96-_x,0 +_s,8)
	 add_build(128-_x*2,16+_s,8)
 end
end

function update_builds()
 if (not build_active) return
	distfact=lerp(distfact,0.1,0.02)
 build_t=t()-build_act_time
	for b in all(builds) do
 	b.y+=1
