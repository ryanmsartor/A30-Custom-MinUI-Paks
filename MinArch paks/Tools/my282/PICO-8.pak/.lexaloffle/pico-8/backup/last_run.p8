end

function bin_2_string(_bin)
 -- five bytes each which is a max of 31
 -- 00000 00001 00010 = cba
 -- oh yeah its also backwards ?? maybe not anymore
 -- alphabet (26) + , . - 
 local c=_bin&31
 local b=(_bin>>5)&31
 local a=(_bin>>10)&31

 local l3=alphabet[min(c,26)]
 local l2=alphabet[min(b,26)]
 local l1=alphabet[min(a,26)]

 if l3==nil and l2==nil and l1==nil then
 	return "aaa"
 end
 return l3..l2..l1
end

function get_alphabet_pos(c)
 local pos=0
	for i=1,#alphabet do
		if alphabet[i]==c then
			return i
		end
	end
	return 1
end

function string_2_bin(str)
 print(str)
 print(#str)
	local c1,c2,c3=
		char_2_bin(
		 get_alphabet_pos(str[1])),
		char_2_bin(
		 get_alphabet_pos(str[2])),
		char_2_bin(
		 get_alphabet_pos(str[3]))
	local b="0b"..c3..c2..c1
	return tonum(b)
end

-- convert 0-31 number to 5 bit binary
function char_2_bin(_num)
 local cur=_num
 local out=""
 for i=0,5 do
  if cur>0 then
   out..=cur%2
   cur=flr(cur*0.5)
  end
 end
 out = sub(out.."00000",1,5)
 local n_out=""
 for i=#out,0,-1 do
  n_out..=sub(out,i,i)
 end
 return n_out
end

